<?xml version="1.0" ?>
<launch>

  <arg name="multiplexer_name"	default="camera_multiplexer"/>
  <arg name="camera_names"	default="a_bot_inside_camera"/>
  <arg name="serial_nos"	default="639204000513, "/>

  <arg name="publish_tf"	default="true"/>
  <arg name="camera_width"	default="640"/>
  <arg name="camera_height"	default="480"/>
  <arg name="fps"		default="30"/>
  <arg name="enable_streaming"	default="true"/>
  <arg name="manager"		default=""/>

  <arg name="external_manager"	value="$(eval manager != '')"/>

  <!-- Launch realsense cameras -->
  <include file="$(dirname)/include/spawn_realsense_cameras.launch">
    <arg name="multiplexer_name"	value="$(arg multiplexer_name)"/>
    <arg name="camera_names"		value="$(arg camera_names)"/>
    <arg name="serial_nos"		value="$(arg serial_nos)"/>
    <arg name="publish_tf"		value="$(arg publish_tf)"/>
    <arg name="camera_width"		value="$(arg camera_width)"/>
    <arg name="camera_height"		value="$(arg camera_height)"/>
    <arg name="fps"			value="$(arg fps)"/>
    <arg name="enable_streaming"	value="$(arg enable_streaming)"/>
    <arg name="manager"			value="$(arg manager)"/>
    <arg name="external_manager"	value="$(arg external_manager)"/>
  </include>

  <node name="$(anon rviz)" pkg="rviz" type="rviz"
  	args="-d $(dirname)/test.rviz" respawn="false" output="screen"/>
  <node name="rqt_reconfigure" pkg="rqt_reconfigure" type="rqt_reconfigure"/>

</launch>
