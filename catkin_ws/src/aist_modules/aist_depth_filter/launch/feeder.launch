<?xml version="1.0" ?>
<launch>
  <arg name="data_dir"		default="~/data/WRS_Dataset"/>
  <arg name="intrinsic"		default="~/data/realsense_intrinsic.json"/>
  <arg name="camera_frame"	default="map"/>

  <node name="feeder"
	pkg="aist_depth_filter" type="image_feeder.py" output="screen">
    <param name="data_dir"	value="$(arg data_dir)"/>
    <param name="intrinsic"	value="$(arg intrinsic)"/>
    <param name="camera_frame"	value="$(arg camera_frame)"/>
  </node>

  <!-- <include file="$(find aist_depth_filter)/launch/run.launch" -->
  <!-- 	   ns="$(arg camera_name)"> -->
  <!--   <arg name="camera_info_topic"	value="color/camera_info"/> -->
  <!--   <arg name="image_topic"		value="color/image_raw"/> -->
  <!--   <arg name="depth_topic"		value="aligned_depth_to_color/image_raw"/> -->
  <!--   <arg name="normal_topic"		value=""/> -->
  <!-- </include> -->

  <arg name="command_args"
       value="-d $(find aist_depth_filter)/launch/feeder.rviz"/>
  <node name="$(anon rviz)" pkg="rviz" type="rviz"
	respawn="false" args="$(arg command_args)" output="screen"/>

  <node name="rqt_reconfigure" pkg="rqt_reconfigure" type="rqt_reconfigure"/>
</launch>
