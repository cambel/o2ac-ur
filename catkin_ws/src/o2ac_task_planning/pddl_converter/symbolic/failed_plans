(pick a_bot panel_bearing b_bot%%%)
(place a_bot panel_bearing base sub-assembly-panel_bearing)
(equip b_bot screw_tool_m4)
(pick-screw b_bot screw_tool_m4)
(fasten-with-screw b_bot screw_tool_m4 panel_bearing panel_bearing/bottom_screw_hole_1 sub-assembly-panel_bearing)
(release a_bot panel_bearing sub-assembly-panel_bearing)
(pick-screw b_bot screw_tool_m4)
(fasten-with-screw b_bot screw_tool_m4 panel_bearing panel_bearing/bottom_screw_hole_2 sub-assembly-panel_bearing)
(finished sub-assembly-panel_bearing)
; cost = 9 (unit cost)
=====
(pick b_bot panel_bearing a_bot)
(place b_bot panel_bearing base sub-assembly-panel_bearing)
(equip a_bot screw_tool_m4)
(pick-screw a_bot screw_tool_m4)
(fasten-with-screw a_bot screw_tool_m4 panel_bearing panel_bearing/bottom_screw_hole_1 sub-assembly-panel_bearing)
(release b_bot panel_bearing sub-assembly-panel_bearing%%%)
(pick-screw a_bot screw_tool_m4)
(fasten-with-screw a_bot screw_tool_m4 panel_bearing panel_bearing/bottom_screw_hole_2 sub-assembly-panel_bearing)
(finished sub-assembly-panel_bearing)
; cost = 9 (unit cost)
